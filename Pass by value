#include <iostream>
using namespace std;


void swapValues(int x, int y) {
    int temp = x;
    x = y;
    y = temp;
}

int main() {
    int num1 = 5, num2 = 2;

    swapValues(num1, num2);

    cout << "num1 = " << num1 << endl;
    cout << "num2 = " << num2 << endl;

    return 0;
}





EXPECTED OUTPUT : 

num1 = 5
num2 = 2
